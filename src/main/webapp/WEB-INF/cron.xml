<?xml version="1.0" encoding="UTF-8"?>
<cronentries>
    <cron>
        <url>/cron/quote</url>
        <description>daily quote</description>
        <schedule>every day 16:00</schedule>
        <retry-parameters>
            <min-backoff-seconds>2.5</min-backoff-seconds>
            <max-doublings>5</max-doublings>
        </retry-parameters>
    </cron>

    <cron>
        <url>/cron/activity</url>
        <description>daily activity</description>
        <schedule>every day 08:00</schedule>
    </cron>

    <cron>
        <url>/cron/database</url>
        <description>init database</description>
        <schedule>every 8 hours</schedule>
    </cron>
</cronentries>
